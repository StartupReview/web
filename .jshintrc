// docs for jshint : http://www.jshint.com/docs/
// options for jshint : http://www.jshint.com/docs/options/

{
  "node": true,
  "mocha": true,
  "esnext": true,
  "undef": true,
  "unused": true,
  "camelcase": true,
  "bitwise": true,
  "eqeqeq": true,
  "freeze": true,
  "strict": false,
  "predef": [
    "angular",
    "$",
    "_",
    "moment",
    "ga",
    "console",
    "twemoji",
    "confirm",
    "document",
    "window",
    "navigator",
    "Rollbar",
    "SC",
    "describe",
    "beforeEach",
    "module",
    "inject",
    "it",
    "spyOn",
    "expect",
    "afterEach",
    "controller",
    "process",
    "FormData",
    "DISQUS"
  ]
}
